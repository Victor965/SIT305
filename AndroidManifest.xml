<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vabolari.login_register" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.vabolari.login_register.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.vabolari.login_register.UserLogin"
            android:label="@string/title_activity_user_login"
            android:parentActivityName="com.example.vabolari.login_register.MainActivity" >
        </activity>
        <activity
            android:name="com.example.vabolari.login_register.UserRegister"
            android:label="@string/title_activity_user_register"
            android:parentActivityName="com.example.vabolari.login_register.MainActivity" >
        </activity>
        <activity
            android:name="com.example.vabolari.login_register.User"
            android:label="@string/title_activity_user" >
        </activity>
        <activity
            android:name="com.example.vabolari.login_register.AboutMe"
            android:label="@string/title_activity_about_me" >
        </activity>
        <activity
            android:name="com.example.vabolari.login_register.video"
            android:label="@string/title_activity_video" >
        </activity>
    </application>

</manifest>
